<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN""http://hibernate.sourceforge.net/hibernate
-mapping-3.0.dtd">

<hibernate-mapping package="model">
	<class name="Catalogo">
		<id name="idCatalogo" column="ID_CATALOGO">
			<generator class="native"/>
		</id>
		<set name="contenidos" cascade="all">
			<key column="ID_CATALOGO" not-null="true" />
			<one-to-many class="Contenido"/>
		</set>
	</class>
</hibernate-mapping>
